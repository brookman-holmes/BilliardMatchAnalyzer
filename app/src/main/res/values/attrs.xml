<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="InkPageIndicator">
        <attr name="dotDiameter" format="dimension" />
        <attr name="dotGap" format="dimension" />
        <attr name="animationDuration" format="integer" />
        <attr name="pageIndicatorColor" format="color" />
        <attr name="currentPageIndicatorColor" format="color" />
    </declare-styleable>

    <declare-styleable name="CueBallView">
        <attr name="hit_indicator" format="dimension" />
        <attr name="hit_indicator_alpha" format="integer" />
        <attr name="line_width" format="dimension" />
        <attr name="hit_color" format="color" />
    </declare-styleable>

    <declare-styleable name="HorizontalBarView">
        <attr name="bar_emptyColor" format="color" />
        <attr name="bar_fillColor" format="color" />
        <attr name="bar_emptyTextColor" format="color" />
        <attr name="bar_fillTextColor" format="color" />
        <attr name="bar_textSize" format="dimension" />
        <attr name="bar_text" format="string" />
        <attr name="bar_textPadding" format="dimension" />
        <attr name="bar_fillPercentage" format="float" />
        <attr name="bar_fontPath" format="string" />
    </declare-styleable>

    <declare-styleable name="HowMissLayout">
        <attr name="hml_title" format="string" />
        <attr name="hml_left_label" format="string" />
        <attr name="hml_right_label" format="string" />
    </declare-styleable>
</resources>