<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:width="24dp"
        android:height="24dp"
        android:bottom="0dp"
        android:left="0dp"
        android:right="0dp"
        android:top="0dp">
        <shape android:shape="oval">
            <solid android:color="@android:color/transparent"/>
        </shape>
    </item>

    <item
        android:width="24dp"
        android:height="14dp"
        android:bottom="5dp"
        android:left="0dp"
        android:right="0dp"
        android:top="5dp">
        <shape android:shape="rectangle">
            <corners android:radius="6dp"/>
        </shape>
    </item>
</layer-list>